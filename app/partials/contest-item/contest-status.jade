div.contest
  div.contest__content
    div.contest__inner
      div.contest__title-section
        h2 Отправки

      div.status-content
        md-content
          md-tabs(md-dynamic-height, md-border-bottom, md-selected="selectedTabIndex")
            md-tab(label='Мои отправки')
              div.sent-list
                div.sent-list__item.sent-list__header
                  div.sent-list__cell
                    span ID
                  div.sent-list__cell
                    span Время
                  div.sent-list__cell
                    span Участник
                  div.sent-list__cell.sent-list__cell-task
                    span Задача
                  div.sent-list__cell
                    span Язык
                  div.sent-list__cell
                    span Вердикт
                  div.sent-list__cell
                    span Номер теста
                  div.sent-list__cell
                    span Время исполнения
                  div.sent-list__cell
                    span Память

                div.sent-list__item(ng-repeat='sent in sents',
                ng-class='{"sent-list__success": sent.verdict_id === 1, "sent-list__failed": [2, 3, 4, 5, 6, 7, 8, 9].indexOf(sent.verdict_id) !== -1, "sent-list__unknown": [10, 11].indexOf(sent.verdict_id) !== -1}')
                  div.sent-list__cell(ng-switch='sent.contestant_id === currentUser.id || currentUser.access_group.access_level === 5')
                    span(ng-switch-when='true')
                      a.link.sent-list__source-link.sent-list__bold(title='Посмотреть решение', ui-sref='contest.source-code({sourceId: sent.sent_id})') {{sent.sent_id}}
                    span(ng-switch-default) {{sent.sent_id}}
                  div.sent-list__cell
                    span {{(sent.sent_time - sent.contest_start_time) | formatStartTime}}
                  div.sent-list__cell
                    span {{sent.user_full_name}}
                  div.sent-list__cell.sent-list__cell-task
                    a.link.sent-list__link(ui-sref='^.conditions-item({problemIndex: sent.internal_index})', title='{{sent.internal_index + ". " + sent.title}}') {{sent.internal_index + '. ' + sent.title}}
                  div.sent-list__cell
                    span {{sent.system_lang_name}}
                  div.sent-list__cell.sent-list__verdict(ng-switch='!sent.verdict_id')
                    span(ng-switch-when='true')
                      div.sent-list__loading
                        div.content-spin__layer
                          div.search-spin-layer_
                            div.search-spin
                    span(ng-switch-default) {{sent.verdict_name}}
                  div.sent-list__cell(ng-switch='sent.test_num === 0')
                    span(ng-switch-when='true') -
                    span(ng-switch-default) {{sent.test_num}}
                  div.sent-list__cell
                    span {{sent.execution_time + ' с'}}
                  div.sent-list__cell
                    span {{sent.memory + ' КБ'}}

                div.no-sents(ng-if='sents.length === 0 && !loadingData')
                  span Отправки отсутствуют

            md-tab(label='Все отправки')
              div.sent-list
                div.sent-list__item.sent-list__header
                  div.sent-list__cell
                    span ID
                  div.sent-list__cell
                    span Время
                  div.sent-list__cell
                    span Участник
                  div.sent-list__cell.sent-list__cell-task
                    span Задача
                  div.sent-list__cell
                    span Язык
                  div.sent-list__cell
                    span Вердикт
                  div.sent-list__cell
                    span Номер теста
                  div.sent-list__cell
                    span Время исполнения
                  div.sent-list__cell
                    span Память

                div.sent-list__item(ng-repeat='sent in sents',
                ng-class='{"sent-list__success": sent.verdict_id === 1, "sent-list__failed": [2, 3, 4, 5, 6, 7, 8, 9].indexOf(sent.verdict_id) !== -1, "sent-list__unknown": [10, 11].indexOf(sent.verdict_id) !== -1}')
                  div.sent-list__cell(ng-switch='sent.contestant_id === currentUser.id || currentUser.access_group.access_level === 5')
                    span(ng-switch-when='true')
                      a.link.sent-list__source-link.sent-list__bold(title='Посмотреть решение', ui-sref='contest.source-code({sourceId: sent.sent_id})') {{sent.sent_id}}
                    span(ng-switch-default) {{sent.sent_id}}
                  div.sent-list__cell
                    span {{(sent.sent_time - sent.contest_start_time) | formatStartTime}}
                  div.sent-list__cell
                    span {{sent.user_full_name}}
                  div.sent-list__cell.sent-list__cell-task
                    a.link.sent-list__link(ui-sref='^.conditions-item({problemIndex: sent.internal_index})', title='{{sent.internal_index + ". " + sent.title}}') {{sent.internal_index + '. ' + sent.title}}
                  div.sent-list__cell
                    span {{sent.system_lang_name}}
                  div.sent-list__cell.sent-list__verdict(ng-switch='!sent.verdict_id')
                    span(ng-switch-when='true')
                      div.sent-list__loading
                        div.content-spin__layer
                          div.search-spin-layer_
                            div.search-spin
                    span(ng-switch-default) {{sent.verdict_name}}
                  div.sent-list__cell(ng-switch='sent.test_num === 0')
                    span(ng-switch-when='true') -
                    span(ng-switch-default) {{sent.test_num}}
                  div.sent-list__cell
                    span {{sent.execution_time + ' с'}}
                  div.sent-list__cell
                    span {{sent.memory + ' КБ'}}

                div.no-sents(ng-if='sents.length === 0 && !loadingData')
                  span Отправки отсутствуют

          include ./pagination